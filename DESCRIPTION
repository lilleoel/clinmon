Type: Package
Package: clintools
Title: Tools for Clinical Research
Version: 0.8.0
Authors@R: c(
    person("Markus Harboe", "Olsen", email = "oel@oelfam.com", role = c("cre","aut")),
    person("Christian", "Riberholt", role = "aut"),
    person("Ronan", "Berg", role = "aut"),
